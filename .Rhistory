getwd()
library(tidyr)
library(rlang)
library(tidyr)
# Do not change these settings!
knitr::opts_chunk$set(warning = FALSE, message = FALSE)
library(readr)
library(ggplot2)
df <- read_csv("National.csv")
colnames(df)[which(names(df) == "X1")] <- "Date"
colnames(df)[which(names(df) == "Daily local cases (bars)")] <- "Daily_cases"
colnames(df)[which(names(df) == "Cumulative local cases (line)")] <- "Cumulative_cases"
df <- transform(df, Date = as.Date(Date, "%d/%m"))
defaultColor = "#1c1c1c"
dailyColor = "dodgerblue3"
cumulativeColor = "#fa8d19"
p <- ggplot(df) +
geom_bar(aes(x=Date, y=Daily_cases, fill="Daily Cases"), stat="identity", alpha=.5) +
scale_fill_manual(NULL, values= c("Daily Cases"=dailyColor)) +
geom_line(aes(x=Date, y=Cumulative_cases/31.43, group=1, linetype = "Cumulative Cases"), stat="identity", color=cumulativeColor, size=1) +
scale_linetype_manual(NULL, values = 1) +
scale_y_continuous(name="Daily Cases", sec.axis = sec_axis(~.*31.43, name="Cumulative Cases")) +
labs(title = "National Locally-Acquired Cases: Daily and Cumulative",
x = "") +
theme_minimal() +
theme(title = element_text(color=defaultColor, size=14),
axis.title.y = element_text(color=defaultColor, size=10, margin = margin(r = 20)),
axis.title.y.right = element_text(color=defaultColor, size = 10, margin = margin(l=20)),
axis.text = element_text(color = defaultColor, size=8),
legend.spacing = unit(-1, "lines"),
legend.position = c(0.14,0.85))
p
